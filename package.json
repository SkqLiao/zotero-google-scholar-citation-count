{
  "name": "zotero-scholar-citations",
  "version": "3.0.1",
  "bugs": {
    "url": "https://github.com/justinribeiro/zotero-scholar-citations/issues"
  },
  "license": "MPLv2",
  "repository": {
    "type": "git",
    "url": "git@github.com:justinribeiro/zotero-scholar-citations.git"
  },
  "scripts": {
    "package": "npm run clean && bash -c ./build.sh",
    "clean": "rm -rf coverage .nyc_output build",
    "test": "jest"
  },
  "devDependencies": {
    "assert": ">=1.4.1",
    "core-js": "^3.18.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.5.0",
    "jest": "^27.2.4",
    "mocha": "^9.1.2",
    "nyc": "^14.1.1",
    "prettier": "2.4.1",
    "run-script-os": ">=1.0.5",
    "sinon": ">=7.3.1",
    "sync-request": ">=6.1.0"
  },
  "prettier": {
    "singleQuote": true
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "plugins": [
      "jest"
    ],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "node": true,
      "jest/globals": true
    },
    "globals": {
      "Zotero": false,
      "ZoteroPane": false,
      "ZoteroStandalone": false
    }
  },
  "private": true
}
