{
  "name": "zotero-scholar-citations",
  "version": "3.0.1",
  "bugs": {
    "url": "https://github.com/justinribeiro/zotero-scholar-citations/issues"
  },
  "license": "MPLv2",
  "repository": {
    "type": "git",
    "url": "git@github.com:justinribeiro/zotero-scholar-citations.git"
  },
  "scripts": {
    "package": "npm run clean && bash -c ./build.sh",
    "clean": "rm -rf coverage .nyc_output build",
    "test": "nyc --reporter=html --reporter=text-summary mocha --ui tdd"
  },
  "devDependencies": {
    "assert": ">=1.4.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "nyc": "^14.1.1",
    "prettier": "2.4.1",
    "run-script-os": ">=1.0.5",
    "sinon": ">=7.3.1",
    "sync-request": ">=6.1.0"
  },
  "prettier": {
    "singleQuote": true
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "env": {
      "browser": true,
      "node": true
    },
    "globals": {
      "Zotero": false,
      "ZoteroPane": false,
      "ZoteroStandalone": false
    }
  },
  "private": true
}
